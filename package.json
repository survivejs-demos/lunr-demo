{
  "name": "lunr-demo",
  "version": "0.0.0",
  "description": "lunr demo showcasing Webpack require.ensure",
  "scripts": {
    "deploy": "gh-pages -d build",
    "stats": "webpack --env build --profile --json > stats.json",
    "build": "webpack --env build",
    "prestart": "npm run generate_index",
    "start": "webpack-dev-server --env start",
    "generate_index": "node ./utils/generate_index.js > app/search_index.json"
  },
  "keywords": [
    "webpack",
    "demo"
  ],
  "author": "Juho Vepsäläinen",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "2.1.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "json-loader": "^0.5.4",
    "remove-markdown": "^0.1.0",
    "style-loader": "^0.14.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "lunr": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
